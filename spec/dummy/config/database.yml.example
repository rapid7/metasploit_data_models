# Please only use postgresql bound to a TCP port.
development: &pgsql
  adapter: postgresql
  database: metasploit_data_models_development
  username: metasploit_data_models_development
  password: __________________________________
  host: localhost
  port: 5432
  pool: 5
  timeout: 5

